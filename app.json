{
  "expo": {
    "name": "Gerador TMP",
    "slug": "gerador-tmp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "gerador-tmp",
    // ADICIONE ESTA LINHA:
    "owner": "talyson020", // Isso é CRUCIAL para o seu URI de redirecionamento do Expo.
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.talyson020.gerador-tmp"
    },
    "android": {
      "package": "com.talyson020.geradortmp",
      "permissions": [
        "INTERNET",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Permitir o uso do FaceID para autenticar o acesso às senhas."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "abd5ff00-a1bc-42dd-bb39-f39204f90d1f"
      }
    }
  }
}
